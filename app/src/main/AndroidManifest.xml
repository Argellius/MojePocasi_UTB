<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mojepocasi_utb">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/the_sun"
        android:label="@string/app_name"
        android:roundIcon="@drawable/the_sun"
        android:supportsRtl="true"
        android:theme="@style/Theme.MojePocasi_UTB">
        <activity android:name=".Activity_VypisPocasi"></activity>
        <activity android:name=".Activity_historie"></activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".Activity_SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>